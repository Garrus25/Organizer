<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #4D57AF;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.organizerclients.RegisterViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="400.0" spacing="10.0" style="-fx-background-color: #d5dde8;">
        <children>
            <Label fx:id="registerText" alignment="CENTER_RIGHT" text="Register">
                <font>
                    <Font size="21.0" />
                </font></Label>
            <TextField fx:id="emailTextField" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="150.0" promptText="Login:" />
            <TextField fx:id="passwordTextField" layoutX="18.0" layoutY="144.0" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="150.0" promptText="Email address:" />
            <Label fx:id="informationText" alignment="CENTER_RIGHT" layoutX="199.0" layoutY="82.0" text="Test" />
            <Button fx:id="registerButton" alignment="CENTER" mnemonicParsing="false" onAction="#onRegisterButtonClick"  prefWidth="110.0" style="-fx-background-color: #5559C9;" text="Register" />
            <Button fx:id="returnButton" alignment="CENTER" layoutX="186.0" layoutY="196.0" mnemonicParsing="false" onAction="#onReturnButtonClick"  prefWidth="110.0" style="-fx-background-color: #1BB30E;" text="Return" />
        </children>
        <VBox.margin>
            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
        </VBox.margin>
        <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
        </padding>
    </VBox>
</VBox>
